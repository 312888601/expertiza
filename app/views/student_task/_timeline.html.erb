  <% @timeline_list.each do |tl| %>
  <li class="li complete">
    <div class="timestamp">
      <% unless tl['link'].nil? %>
      <span class="author"><%=link_to 'link', tl['link'] %></span>
      <% else %>
      <span class="author">-</span>
      <% end %>
      <span class="date"><%= tl['updated_at'] %><span>
    </div>
    <div class="status">
      <h4><%= tl['label'] %></h4>
    </div>
  <% end %>
